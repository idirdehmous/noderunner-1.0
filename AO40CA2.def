    family 21
      defstatus complete
      edit RR '21'
      edit YMD ''
      edit RUNDATE '@YMD@@RR@'
      edit FCLEN '3'
      edit THIS_RUN '21'
      edit PREV_RUN '18'
      edit NEXT_RUN '00'
      label RUNDATE ""
      family initialisation
        trigger ../18/next_cycle/save_first_guess/03 == complete 
        task init_forecast
          edit HOST 'hpc'
          edit USER 'cvah'
          edit ECF_OUT '/home/cvah/ECF/AO40CA2/tmp'
          edit LOCALJOB 'yes'
        task copy_namelists
          edit HOST 'hpc'
          edit USER 'cvah'
          edit ECF_OUT '/home/cvah/ECF/AO40CA2/tmp'
          edit LOCALJOB 'yes'
          inlimit max_init
        task sync_data
          inlimit max_init
        task init_workpaths
      endfamily
      family lbc
        trigger ./initialisation == complete
        edit ECF_FILES '/home/cvah/ECF/AO40CA2/scr/lbc'
        inlimit max_postproc
        event LAUNCH_FC
        event LAUNCH_00
        task retrieve_lbc
          edit WALLTIME '@WALLTIME_RET@'
          meter lbc_counter -1 3 3
        task prep_lbc
          trigger retrieve_lbc == complete
          edit WALLTIME '@WALLTIME_PRE@'
          edit NODE_SELECT '@SELECT_PRE@'
          meter lbc_counter -1 3 3
      endfamily
      family assimilation
        trigger ./initialisation == complete
        edit NODE_SELECT '@SELECT_SERIAL@'
        edit WALLTIME '@WALLTIME_SERIAL@'
        task check_first_guess
        family surface
          trigger ./check_first_guess == complete
          edit OBSTYPES '@OBSTYPES_SURFACE@'
          edit ASSIM_LABEL 'surface'
          task get_obs
            edit ECF_FILES '/home/cvah/ECF/AO40CA2/scr/assimilation'
          task bator
            trigger ./get_obs == complete
            edit ECF_FILES '/home/cvah/ECF/AO40CA2/scr/assimilation'
            edit NODE_SELECT '@SELECT_BATOR@'
            edit WALLTIME '@WALLTIME_BATOR@'
          task odb_merge
            trigger ./bator == complete
            edit ECF_FILES '/home/cvah/ECF/AO40CA2/scr/assimilation'
          task sst_update
            trigger (../../lbc == complete or ../../lbc:LAUNCH_00 == set)
          task canari
            trigger ./odb_merge == complete and ./sst_update == complete
            edit NODE_SELECT '@SELECT_CANARI@'
        endfamily
        family upper_air
          trigger ./surface == complete
          task copy_soil
            edit NODE_SELECT '@SELECT_POS@'
            edit WALLTIME '@WALLTIME_POS@'
        endfamily
        task archive_assim
          trigger ./surface == complete && ./upper_air == complete
      endfamily
      family forecast
        trigger (./lbc == complete or ./lbc:LAUNCH_FC == set) && ./assimilation == complete 
        task integration
          edit ECF_FILES '/home/cvah/ECF/AO40CA2/scr/forecast'
          edit NODE_SELECT '@SELECT_FC@'
          edit WALLTIME '@WALLTIME_FC@'
        task monitor
          trigger ./integration == active || ./integration == complete
          edit NODE_SELECT '@SELECT_SERIAL@'
          edit WALLTIME '@WALLTIME_FC@'
          meter forecast_counter -1 3 3
      endfamily
      family next_cycle
        task queue_next
          complete /AO40CA2/cycle/00 == active and (100 * /AO40CA2/cycle/00:YMD + /AO40CA2/cycle/00:RR > 100 * :YMD + :RR)
          trigger ../initialisation == complete && /AO40CA2/cycle/00 == complete
          edit HOST 'hpc'
          edit USER 'cvah'
          edit ECF_OUT '/home/cvah/ECF/AO40CA2/tmp'
          edit LOCALJOB 'yes'
        family save_first_guess
          family 03
            trigger ../../forecast/monitor:forecast_counter >= 3
            edit WALLTIME '00:05:00'
            edit ECF_FILES '/home/cvah/ECF/AO40CA2/scr/next_cycle'
            task save_first_guess
          endfamily
        endfamily
      endfamily
      family finish
        trigger ./forecast == complete
        task cleanup
        task set_defstatus
          edit HOST 'hpc'
          edit USER 'cvah'
          edit ECF_OUT '/home/cvah/ECF/AO40CA2/tmp'
          edit LOCALJOB 'yes'
        task cycle_data_update
          inlimit max_sync
      endfamily
